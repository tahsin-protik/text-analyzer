  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production tsx dist/src/app.js",
    "build": "NODE_ENV=production tsc",
    "dev": "tsx watch src/app.ts",
    "test": "NODE_ENV=test mocha --require tsx 'test/**/*.test.ts'",
    "test:coverage": "NODE_ENV=test c8 mocha --require tsx 'test/**/*.test.ts' && open-cli coverage/index.html",
    "coverage:open": "open-cli coverage/index.html",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all"
  },
